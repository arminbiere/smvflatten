MODULE main
VAR
  a[0] : boolean;
  a[1] : boolean;
  a[2] : boolean;
  b : 0..3;
TRANS
  next(b) = 
  case
    a[0] : 0;
    a[1] : 1;
    a[2] : 0;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 0;
    a[1] : 1;
    a[2] : 3;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 0;
    a[1] : 2;
    a[2] : 0;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 0;
    a[1] : 2;
    a[2] : 3;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 1;
    a[1] : 1;
    a[2] : 0;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 1;
    a[1] : 1;
    a[2] : 3;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 1;
    a[1] : 2;
    a[2] : 0;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 1;
    a[1] : 2;
    a[2] : 3;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 2;
    a[1] : 1;
    a[2] : 0;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 2;
    a[1] : 1;
    a[2] : 3;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 2;
    a[1] : 2;
    a[2] : 0;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 2;
    a[1] : 2;
    a[2] : 3;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 3;
    a[1] : 1;
    a[2] : 0;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 3;
    a[1] : 1;
    a[2] : 3;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 3;
    a[1] : 2;
    a[2] : 0;
    1 : b;
  esac | next(b) = 
  case
    a[0] : 3;
    a[1] : 2;
    a[2] : 3;
    1 : b;
  esac
